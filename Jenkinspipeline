pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                sh 'mkdir build'
                sh 'touch build/readme.txt'
                sh 'echo "test 1..2..3"> build/readme.txt'
            }
        }
        stage('Test') { 
            steps {
                echo "Test" 
            }
        }
        stage('Deploy') { 
            steps {
                echo"deploy" 
            }
        }
    }
}