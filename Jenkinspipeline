pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
               bat 'dir'
               bat 'mkdir file'
               //echo "build"
            }
        }
        stage('Test') { 
            steps {
                echo "Test" 
            }
        }
        stage('Deploy') { 
            steps {
                echo"deploy" 
            }
        }
    }
}